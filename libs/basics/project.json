{
  "projectType": "library",
  "sourceRoot": "libs/basics/src",
  "prefix": "ng-peaqock",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/basics"],
      "options": {
        "jestConfig": "libs/basics/jest.config.ts",
        "passWithNoTests": true
      }
